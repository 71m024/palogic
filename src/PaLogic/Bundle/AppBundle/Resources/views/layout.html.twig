<!DOCTYPE HTML>
<html lang="en-US">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
        <meta name="description" content="{% block description %}PaLogic{% endblock %}">
        <title>{% block title %} - PaLogic{% endblock %}</title>
        <link rel="shortcut icon" type="image/x-icon" href="/style/images/favicon.png?v=2" />
        <!--[if IE 8]>
        <link rel="stylesheet" type="text/css" href="/webpaint/style/css/ie8.css" media="all" />
        <![endif]-->
        <!--[if IE 9]>
        <link rel="stylesheet" type="text/css" href="/webpaint/style/css/ie9.css" media="all" />
        <![endif]-->
        {% stylesheets filter='cssrewrite'
            'bundles/palogicapp/css/style.css'
            '@PaLogicAppBundle/Resources/public/css/media-queries.css'
            '@PaLogicAppBundle/Resources/public/js/fancybox/jquery.fancybox.css'
            '@PaLogicAppBundle/Resources/public/js/fancybox/helpers/jquery.fancybox-thumbs.css'
            'bundles/palogicapp/type/fontello.css'
            '@PaLogicAppBundle/Resources/public/css/stylesheet.css'
            output='generated/style/palogicStylesheet.css'
        %}
            <link rel="stylesheet" media="screen" href="{{ asset_url }}" />
        {% endstylesheets %}
        <link rel="stylesheet" media="screen" href="http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic" />
        {% block stylesheets %}
            {% stylesheets filter='cssrewrite'
                '@PaLogicAppBundle/Resources/public/css/color/yellow.css'
                output='generated/style/palogicStandaloneStylesheet.css'
            %}
                <link rel="stylesheet" media="screen" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
                
        {% block javascriptstop %}
            
        {% endblock %}
                
    </head>
    <body class="full-layout">
        {% include 'PaLogicAppBundle:Google:analyticstracking.html.twig' %}
        <div class="body-wrapper"> 
            <div class="top-wrapper">
                <header>
                    <div class="inner">
                        
                        <div class="logo">
                        {% block blog_title %}
                            <span style="font-size: 25px; font-weight: bold;"><span style="color:#ffc360">PaLogic</span></span>
                        {% endblock %}
                        </div>

                        {% block navigation %}
                            {{ knp_menu_render('main', {'depth': 2, 'currentAsLink': true, 'currentClass' : 'active'}) }}
                        {% endblock %}

                        <div class="clear"></div>
                    </div>
                </header>
                
                {% block banner %}{% endblock %}
                
            </div>
            
            {% block top %}{% endblock %}
            
            <div class="light-wrapper"> 
                <div class="inner">
                    {% block body %}{% endblock %}
                    {% block sidebar %}{% endblock %}
                    <div class="clear"></div>
                </div>
            </div>
            
            {% block bottom %}{% endblock %}

            {% block footer %}
                <footer class="aligncenter">
                    <div class="inner">
                        <h1>Kontakt</h1>
                        <p class="description">Bei Fragen oder Interesse helfen wir ihnen gerne weiter.</p>
                        <ul class="contact-info">
                            <li><i class="icon-location"></i>3053 Deisswil b.M</li>
                            <li><i class="icon-phone"></i><a href="tel:+41792660774">+41 79 266 07 74</a></li>
                            <li><i class="icon-mail"></i> <a href="mailto:info@boxenmieten.ch">info@boxenmieten.ch</a></li>
                        </ul>
                        <ul class="social">
                            <li><a target="_blank" href="https://www.facebook.com/boxenmieten"><i class="icon-s-facebook"></i></a></li>
                            <li><a target="_blank" href="https://twitter.com/boxenmieten"><i class="icon-s-twitter"></i></a></li>
                            <li><a target="_blank" href="https://plus.google.com/+BoxenmietenCh/posts"><i class="icon-s-gplus"></i></a></li>
                            <li><a target="_blank" href="https://github.com/71m024/boxenmieten"><i class="icon-s-github"></i></a></li>
                        </ul>
                    </div>
                </footer>
            {% endblock %}
            
            
            {% block subfooter %}
                <div class="subfooter-wrapper"> 
                    <div class="inner">
                        <p>Â© 2014 Boxenmieten.ch</p>
                    </div>
                </div>
            {% endblock subfooter %}

        </div>
        
        {% block javascripts %}
            <!--[if lt IE 9]>
            <script src="/webpaint/style/js/html5.js" type="text/javascript"></script>
            <![endif]-->
            {% javascripts
                'webpaint/style/js/jquery.min.js'
                'webpaint/style/js/ddsmoothmenu.js'
                'webpaint/style/js/jquery.isotope.min.js'
                'webpaint/style/js/selectnav.js'
                'webpaint/style/js/jquery.easytabs.min.js'
                'webpaint/style/js/jquery.fitvids.js'
                'webpaint/style/js/jquery.fancybox.pack.js'
                'webpaint/style/js/fancybox/helpers/jquery.fancybox-thumbs.js'
                'webpaint/style/js/fancybox/helpers/jquery.fancybox-media.js'
                'webpaint/style/js/jquery.themepunch.plugins.min.js'
                'webpaint/style/js/jquery.themepunch.revolution.min.js'
                'webpaint/style/js/jquery.touchcarousel-1.2.min.js'
                'webpaint/style/js/boostrapslider.js'
                'webpaint/style/js/scripts.js'
                output='generated/scripts/baseScript.js'
            %}
                <script type="text/javascript" src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>